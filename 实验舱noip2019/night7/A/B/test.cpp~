#include <bits/stdc++.h>
using namespace std;
inline int Rand() {
    return rand()|rand()<<16;
}
int main() {
    freopen("B.in","w",stdout);
    srand(((unsigned long long)new char)+time(NULL));
    int n=100,G=2;
    cout<<n<<endl;
    for(int i=1;i<=n;i++) putchar(rand()%G+'a');
    return 0;
}
